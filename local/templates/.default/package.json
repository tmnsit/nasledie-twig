    "scripts": {
        "dev": "webpack --colors --progress",
        "prod": "cross-env NODE_ENV=production webpack --colors --progress",
        "build": "npm run prod",
        "watch": "npm run dev -- --watch",
        "start": "npm i ; npm run watch",
        "postinstall": "npm run build",
        "rm:html": "rm -r ../../../dist/*.html",
        "rm:assets": "rm -r../../../dist/",
        "copy:assets": "cp -r frontend/dist ../../../ ; cp ../../modules/uplab.core/include/tpl/frontend/readme.md ../../../dist",
        "copy:tpl": "cp -r frontend/src/include ../../../dist/",
        "copy": "npm run rm:assets ; npm run copy:assets && npm run rm:html ; npm run copy:tpl",
        "update:front": "cd frontend/ && git pull && npm i && npm run build",
        "update": "npm run update:front && npm run copy && npm i && git status"
